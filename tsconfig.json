{
  "version": "2.3.2",
  "compilerOptions": {
    "module": "commonjs",
    "target": "es5",
    "jsx": "react",
    "sourceMap": true,
    "emitDecoratorMetadata": true,
    "experimentalDecorators": true,
    "strict": true,
    "strictNullChecks": true,
    "outDir": "build",
    "noUnusedLocals": true,
    "lib": ["es2015", "dom"],
  },
  "exclude": [
    "build",
    "config",
    "node_modules",
    "dev_server.js"
  ]
}
